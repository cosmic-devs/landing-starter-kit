{
  "name": "gulp-with-tailwind",
  "version": "1.0.0",
  "description": "Gulp with Tailwind CSS Utility",
  "main": "gulpfile.js",
  "scripts": {
    "dev": "gulp",
    "build": "gulp build",
    "tinypng": "gulp tinypng"
  },
  "options": {
    "config": {
      "tailwindjs": "./tailwind.config.js",
      "port": 3000,
      "webp": {
        "quality": 90
      },
      "tinypng": {
        "key": ""
      }
    },
    "paths": {
      "root": "./",
      "src": {
        "base": "./src",
        "css": "./src/css",
        "js": "./src/js",
        "img": "./src/img",
        "fonts": "./src/fonts",
        "videos": "./src/videos"
      },
      "dist": {
        "base": "./dist",
        "css": "./dist/css",
        "js": "./dist/js",
        "img": "./dist/img",
        "fonts": "./dist/fonts",
        "videos": "./dist/videos"
      },
      "build": {
        "base": "./build",
        "css": "./build/css",
        "js": "./build/js",
        "img": "./build/img",
        "fonts": "./build/fonts",
        "videos": "./build/videos"
      }
    }
  },
  "keywords": [
    "gulp",
    "tailwind",
    "tailwindcss"
  ],
  "author": "Manjunath G",
  "license": "ISC",
  "devDependencies": {
    "alpinejs": "^2.2.1",
    "autoprefixer": "^9.7.4",
    "browser-sync": "^2.27.10",
    "del": "^5.1.0",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-concat": "^2.6.1",
    "gulp-imagemin": "^7.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-purgecss": "^2.1.0",
    "gulp-purify-css": "^0.1.0",
    "gulp-purifycss": "^0.2.0",
    "gulp-replace": "^1.0.0",
    "gulp-sass": "^4.0.2",
    "gulp-terser": "^1.2.0",
    "gulp-tinypng-extended": "^2.0.2",
    "gulp-webp": "^4.0.1",
    "log-symbols": "^3.0.0",
    "purifycss-extended": "^1.3.6",
    "tailwindcss": "^1.2.0",
    "tailwindcss-typography": "^3.0.1"
  },
  "dependencies": {}
}
